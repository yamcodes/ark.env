{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": [".env", ".env.local"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"],
			"env": ["SENTRY_*"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"clean": {
			"cache": false
		},
		"//#fix": {
			"cache": false
		}
	}
}
