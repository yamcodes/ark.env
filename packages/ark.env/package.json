{
	"name": "ark.env",
	"type": "module",
	"version": "0.1.1",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"description": "Typesafe environment variables parsing and validation for Node.js with ArkType",
	"exports": {
		"types": "./dist/index.d.ts",
		"import": "./dist/index.js",
		"require": "./dist/index.cjs"
	},
	"scripts": {
		"build": "rimraf dist && tsup",
		"prepack": "bun run build",
		"sample": "bun --watch src/sample",
		"sample:once": "bun src/sample",
		"test": "bun test --watch",
		"test:once": "bun test",
		"typecheck": "tsc --noEmit",
		"clean": "rimraf dist node_modules"
	},
	"files": ["dist"],
	"keywords": ["bun"],
	"license": "MIT",
	"homepage": "https://github.com/yamcodes/ark.env#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yamcodes/ark.env.git"
	},
	"bugs": "https://github.com/yamcodes/ark.env/issues",
	"author": "Yam Borodetsky <yam@yam.codes>",
	"devDependencies": {
		"bun-plugin-dts": "^0.3.0",
		"rimraf": "^6.0.1",
		"tsup": "^8.3.6",
		"typescript": "^5.7.3"
	},
	"peerDependencies": {
		"arktype": "^2.0.2"
	},
	"dependencies": {
		"picocolors": "^1.1.1"
	}
}
